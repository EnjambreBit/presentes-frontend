
{{#leaflet-map lat=latitud lng=longitud zoom=zoom}}
  {{tile-layer url="https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png"}}
    {{#each model.data as |m|}}
      {{#marker-layer lat=m.latitud lng=m.longitud onClick=(action "accionDePrueba" m) icon=categoryIcon as |marker|}}
        {{presentes-ui/mapa-popup
          nombre=m.nombreCompleto
          fecha=m.fechaDelHecho
          localidad=m.localidad
          provincia=m.provincia
          id=m.id
          ficha=true
          nota=m.linkDeNota
          categoria=m.categoria
          copete=m.copete
          modelo=m
        }}
      {{/marker-layer}}
    {{/each}}
{{/leaflet-map}}
